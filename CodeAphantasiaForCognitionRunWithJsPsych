      const jsPsych = initJsPsych({
        on_finish: () => {
          const display = jsPsych.getDisplayElement();
          display.innerHTML = `
            <div style="text-align: center; max-width: 600px; margin: 0 auto; padding: 20px;">
              <h3 style="color: #28a745;">
                Vos données ont été enregistrées avec succès !
              </h3>
              <p style="margin: 20px 0;">
                Voici le code à copier-coller sur Prolific pour votre payement : <strong>C1NOA6BK</strong>
              </p>
              <p style="font-size: 0.9em; color: #6c757d;">
                Vous pourrez ensuite fermer cette fenêtre (ainsi que celle de l’environnement virtuel le cas échéant).
              </p>
            </div>`;
        }
      });

      var timeline = [];
      
      const preload = {
          type: jsPsychPreload,
          images: ['planflou.png']
      };
      timeline.push(preload);

      var fullscreen = {
        type: jsPsychFullscreen,
        fullscreen_mode: true,
        message: "L'expérience passera en mode plein écran lorsque vous appuierez sur le bouton ci-dessous<br><br><br>", //Translation of default aka english value
        button_label: "Continuer" //in English, "Continue" by default
      };
      timeline.push(fullscreen);


      var welcome = {
        type: jsPsychHtmlKeyboardResponse,       
        stimulus: "Bienvenue dans l'expérience ! Appuyez sur n'importe quelle touche pour commencer"
      };
      timeline.push(welcome);

window.blockCount = 0;
window.shouldStop = false;
window.shouldSkipTests = false;
window.foundWords = []; 
window.temporalSeqCorrectCounter = 0;
window.temporalRoomCorrectCounter = 0;
window.spatialEgoCorrectCounter = 0;
window.spatialAlloCorrectCounter = 0;
window.temporalCorrectCounter = 0;
window.spatialCorrectCounter = 0;
window.objectDetailCorrectCounter = 0;
window.gender = 0
window.age = 0
window.perm = 0
window.currentPerm = false



      var participant_info = {
        type: jsPsychSurveyText,
        questions: [
          {
            prompt: "Quel est votre genre ?",
            placeholder: "_ _ _ _ _",
            name: "Genre",
            required: true,
          },
          {
            prompt: "Quel est votre âge (indiquez seulement un nombre) ?",
            placeholder: "00",
            name: "Age",
            required: true,
          },
          {
            prompt: "Quels sont les deux derniers chiffres de votre numéro de téléphone ?",
            placeholder: "00",
            name: "Permutation",
            required: true,
          },
        ],
        button_label: "Continuer",
        on_finish: function(data) {
    const genreResponse = data.response.Genre.toLowerCase().trim();
    
    const termesMasculins = ["homme", "masculin", "male", "garçon", "monsieur", "m", "h"];
    const termesFeminins = ["femme", "féminin", "female", "fille", "madame", "mademoiselle", "f"];
    
    let minDistanceMasculin = Infinity;
    let minDistanceFeminin = Infinity;
    let termeMasculinLePlusProche = "";
    let termeFemininLePlusProche = "";
    
    for (const terme of termesMasculins) {
      const distance = levenshtein(genreResponse, terme);
      if (distance < minDistanceMasculin) {
        minDistanceMasculin = distance;
        termeMasculinLePlusProche = terme;
      }
    }
    
    for (const terme of termesFeminins) {
      const distance = levenshtein(genreResponse, terme);
      if (distance < minDistanceFeminin) {
        minDistanceFeminin = distance;
        termeFemininLePlusProche = terme;
      }
    }

    const SEUIL_DISTANCE = 2;
    
    let genreDetecte;
    if (minDistanceMasculin <= SEUIL_DISTANCE && minDistanceMasculin < minDistanceFeminin) {
      genreDetecte = "Homme";
    } else if (minDistanceFeminin <= SEUIL_DISTANCE && minDistanceFeminin < minDistanceMasculin) {
      genreDetecte = "Femme";
    } else {
      genreDetecte = "Autre";
    }
    
    data.genreDetecte = genreDetecte;
    data.age_numeric = parseInt(data.response.Age);
    data.permutation_numeric = parseInt(data.response.Permutation);
    window.gender = genreDetecte;
    window.age = parseInt(data.response.Age);
    window.perm = parseInt(data.response.Permutation)
  }
};
      timeline.push(participant_info);

      var general_instructions = {
        type: jsPsychHtmlButtonResponse,
        stimulus: "<p><b>Jeu d'association</b></p><br>" +
        "<p>Vous allez maintenant participer à un jeu d'association où l'objectif est de restituer le plus de mots vus dans l'environnement virtuel.</p><br>" +
        "<p>Pour chaque mot rappelé, des questions sur le contexte environnemental seront posées. Ces questions porteront sur des détails comme l'emplacement ou le moment où le mot est apparu.</p><br>" +
        "<p>Prenez simplement le temps de vous remémorer les mots à votre rythme. Si un mot ne vous revient pas précisément, vous pouvez tenter une orthographe approximative.</p><br>" +
        "<p>Lorsque vous ne vous souvenez plus d'aucun mot, vous pourrez l'indiquer et passer à la suite de l'expérience.</p><br>",
        choices: ["Continuer"],
        data: {screen: "general_instructions"},
      };
      //timeline.push(general_instructions);

function levenshtein(a, b) {
    const matrix = [];
    for(let i = 0; i <= b.length; i++) matrix[i] = [i];
    for(let j = 0; j <= a.length; j++) matrix[0][j] = j;

    for(let i = 1; i <= b.length; i++) {
        for(let j = 1; j <= a.length; j++) {
            const cost = a[j-1] === b[i-1] ? 0 : 1;
            matrix[i][j] = Math.min(
                matrix[i-1][j] + 1,
                matrix[i][j-1] + 1,
                matrix[i-1][j-1] + cost
            );
        }
    }
    return matrix[b.length][a.length];
}


const permCall = {
  type: jsPsychCallFunction,
  func: () => {
    const phoneDigits = window.perm;
    window.currentPerm = permutations[phoneDigits % 100];
  }
};
timeline.push(permCall);

const freeRecallTrial = {
  type: jsPsychSurveyText,
  questions: [{
    prompt: function() {
      let promptText = "";
      
      if (window.shouldSkipTests) {
        promptText += `
          <div style="padding: 10px; margin-bottom: 15px; background-color: #f8d7da; border: 1px solid #f5c6cb; border-radius: 5px; color: #721c24;">
            <p><i>Ce qui a été entré est un peu trop proche d'un mot précédemment entré ou n'a pas été reconnu<i></p>
            <p>Veuillez essayer de vous souvenir d'un autre mot ou de l'écrire autrement.</p>
          </div>
        `;
      }
      
      promptText += `<p><strong>Écrivez un (1) des mots affichés lors de votre navigation dont vous vous souvenez :</strong></p>` +
      "<p>L'orthographe exacte n'est pas requise. Écrivez simplement le mot comme vous vous en souvenez.<p>";
      
      if (window.blockCount >= 1) {
        promptText += `
          <p style="font-size:0.85em; color:#555; margin-bottom:10px;">
            Si vous ne vous souvenez plus d'aucun mot, écrivez "<strong>STOP</strong>" dans le champ ci-dessous.
            <br><i>Cela mettra fin prématurément à l'expérience<i>
          </p>`;
      }
      
      return promptText;
    },
    name: "free_recall",
  }],
  button_label: "Continuer",
  on_finish: data => {
    const answer = data.response.free_recall.trim().toLowerCase();
    
    if (answer === "stop") {
      window.shouldStop = true;
      data.did_stop = true;
      return;
    }
    
    data.did_stop = false;
    
    let minDist = Infinity, closestIdx = -1, closestWord = "";
    window.currentPerm.forEach((w, i) => {
      const d = levenshtein(answer, w.toLowerCase());
      if (d < minDist) {
        minDist = d; closestIdx = i; closestWord = w;
      }
    });
    
    data.correct_word = closestWord;
    data.levenshtein = minDist;
    data.perm_position = closestIdx;
    
    if (minDist > 5) {
      data.word_recognized = false;
      data.skip_reason = "distance_too_large";
      
      window.shouldSkipTests = true;
    } 
    else if (window.foundWords.includes(closestWord)) {
      data.word_recognized = true;
      data.word_already_found = true;
      data.skip_reason = "word_already_found";
      
      window.shouldSkipTests = true;
    } 
    else {
      data.word_recognized = true;
      data.word_already_found = false;
      data.skip_reason = null;
      
      window.foundWords.push(closestWord);
      
      window.correctWord = closestWord;
      window.permPosition = closestIdx;
      window.shouldSkipTests = false;
    }
  }
};
//timeline.push(freeRecallTrial);

const temporalseqBindingTest = {
    type: jsPsychSurveyMultiChoice,
    questions: [
        {
            prompt: "Lors de votre exploration, ce mot est-il apparu plutôt au début ou à la fin du parcours ?",
            name: "temporal_sequence_response",
            options: ["Première moitié", "Seconde moitié", "<i>Je ne sais pas<i>"],
            required: true
        }
    ],
    button_label: "Continuer",
    on_finish: (data) => {
        const correctTemporalSeq = window.permPosition < 22 ? "Première moitié" : "Seconde moitié";
        data.temporal_seq_correct = data.response.temporal_sequence_response === correctTemporalSeq;
        data.temporal_seq_correct_answer = correctTemporalSeq;
        data.temporal_seq_skipped = data.response.temporal_seq_response === "<i>Je ne sais pas<i>";
    if (data.temporal_seq_correct) {
      window.temporalSeqCorrectCounter++;
    }
    data.cumulative_temporal_seq_correct = window.temporalSeqCorrectCounter;

    }
};

const temporalroomBindingTest = {
    type: jsPsychSurveyMultiChoice,
    questions: [
        {
            prompt: "Dans quelle pièce avez-vous vu ce mot ?",
            name: "temporal_room_response",
            options: ["Cuisine", "Salon", "Chambre", "Salle de bain", "<i>Je ne sais pas<i>"],
            required: true
        }
    ],
    button_label: "Continuer",
    on_finish: (data) => {

        const currentIndex = window.currentPerm.indexOf(window.correctWord);
        let correctRoom = "";
        if(currentIndex < 11) correctRoom = "Cuisine";
        else if(currentIndex < 22) correctRoom = "Salon";
        else if(currentIndex < 33) correctRoom = "Chambre";
        else correctRoom = "Salle de bain";
        
    const response = data.response.temporal_room_response;
    data.room_correct = (response === correctRoom);
    data.room_correct_answer = correctRoom;
    data.room_skipped = (response === "<i>Je ne sais pas<i>");
    if (data.room_correct) {
      window.temporalRoomCorrectCounter++;
    }
    data.cumulative_room_correct = window.temporalRoomCorrectCounter;
    }
};
//timeline.push(temporalBindingTest);


window.spatialPosition = ["face","face","droite","droite","droite","droite","droite","face","face","face","droite","gauche","face","face","face","gauche","face","face","gauche","gauche","face","face","gauche","gauche","gauche","gauche","face","gauche","gauche","face","face","face","droite","face","droite","droite","face","gauche","gauche","gauche","face","face","face","droite"];

const spatialBindingTest = {
    type: jsPsychSurveyMultiChoice,
    questions: [
        {
            prompt: "Le mot était-il positionné à gauche, en face ou à droite du corps de l'avatar ?",
            name: "spatial_response",
            options: ["À gauche", "En face", "À droite", "<i>Je ne sais pas<i>"],
            required: true
        }
    ],
    button_label: "Continuer",    
    on_finish: (data) => {
        const correctPos = window.spatialPosition[window.permPosition];
        
        const correctAnswer = {
            "gauche": "À gauche",
            "droite": "À droite",
            "face": "En face"
        }[correctPos];
        
        data.spatial_correct = data.response.spatial_response === correctAnswer;
        data.spatial_correct_answer = correctAnswer;
        data.spatial_skipped = data.response.spatial_response === "<i>Je ne sais pas<i>";
        if (data.spatial_correct) {
          window.spatialEgoCorrectCounter++;
        }
        data.cumulative_spatialego_correct = window.spatialEgoCorrectCounter;

        
    }
};
//timeline.push(spatialBindingTest);

const spatialAlloBindingTest = {
  type: jsPsychHtmlKeyboardResponse,
  stimulus: function() {
    return `<div style="position: relative; width: 400px; margin: 0 auto;">
      <p>Sur cette carte floutée vue du dessus de l'environnement virtuel, cliquez à l'endroit où vous pensez avoir vu apparaître le mot.</p>
      <div id="image-container" style="position: relative; width: 400px; height: 364px;">
        <img src="planflou.png" style="width: 100%; height: 100%;" id="clickable-image">
        <div id="marker" style="position: absolute; width: 10px; height: 10px; border-radius: 50%; background-color: red; transform: translate(-50%, -50%); display: none;"></div>
      </div>
      <button id="continue-button" style="margin-top: 10px; padding: 5px 10px; display: none;">Continuer</button>
    </div>`;
  },
  choices: "NO_KEYS",
  on_load: function() {
    const img = document.getElementById('clickable-image');
    const marker = document.getElementById('marker');
    const continueBtn = document.getElementById('continue-button');

    if (!img || !continueBtn) {
      console.error("Elements not found");
      return;
    }
    
    img.onerror = function() {
      img.style.display = "none";
      const container = document.getElementById('image-container');
      container.innerHTML += '<div style="width: 100%; height: 100%; display: flex; align-items: center; justify-content: center; background-color: #f0f0f0;">Image non disponible</div>';
      continueBtn.style.display = 'block';
    };
    
    let clickX, clickY;
    
    img.addEventListener('click', function(e) {
      const rect = img.getBoundingClientRect();
      clickX = e.clientX - rect.left;
      clickY = e.clientY - rect.top;
      
      marker.style.left = clickX + 'px';
      marker.style.top = clickY + 'px';
      marker.style.display = 'block';
      
      continueBtn.style.display = 'block';
    });
    
    continueBtn.addEventListener('click', function() {
      const trial_data = {
        click_x: clickX,
        click_y: clickY
      };
      
      jsPsych.finishTrial(trial_data);
    });
  },
  data: function() {
    return {
      task: 'spatial_binding_test',
      permPosition: window.permPosition || -1  
    };
  },
  on_finish: (data) => {
    if (data.click_x !== undefined && data.click_y !== undefined) {
        const scaleX = 1, scaleY = 1;
        const scaledX = data.click_x * scaleX;
        const scaledY = data.click_y * scaleY;
        
        const objPositions = [
          {x: 60,  y: 18},  
          {x: 44,  y: 40}, 
          {x: 20,  y: 90},
          {x: 18,  y:110},
          {x: 18,  y:130},
          {x: 14,  y:144},
          {x: 14,  y:170},
          {x: 10,  y:200},
          {x: 66,  y:203},
          {x:117,  y:187},
          {x:127,  y:154}, 
          {x:183,  y: 83}, //12
          {x:240,  y: 30},
          {x:305,  y:106},
          {x:268,  y: 68},
          {x:300,  y:  8},
          {x:364,  y: 42},
          {x:350,  y:140},
          {x:353,  y:172},
          {x:343,  y:203},
          {x:250,  y:174},
          {x:268,  y:208}, 
          {x:353,  y:241}, //23
          {x:351,  y:275},
          {x:343,  y:285},
          {x:386,  y:297},
          {x:348,  y:329},
          {x:301,  y:321},
          {x:280,  y:344},
          {x:277,  y:319},
          {x:259,  y:303}, 
          {x:268,  y:231},
          {x:273,  y:252},
          {x:191,  y:289}, //34
          {x:239,  y:245},
          {x:237,  y:221},
          {x:132,  y:237},
          {x:151,  y:214},
          {x:160,  y:195},
          {x:151,  y:176},
          {x:172,  y:150},
          {x:191,  y:155},
          {x:211,  y:151},
          {x:213,  y:187}   
        ];
        
        const calculateDistance = function(point1, point2) {
          return Math.sqrt(
            Math.pow(point1.x - point2.x, 2) + 
            Math.pow(point1.y - point2.y, 2)
          );
        };
        
        const permPosition = window.permPosition !== undefined ? window.permPosition : -1;
        const correctPos = permPosition >= 0 && permPosition < objPositions.length 
          ? objPositions[permPosition] 
          : {x: 0, y: 0};
        
        const distance = calculateDistance({x: scaledX, y: scaledY}, correctPos);
        
        
        const clickPoint = { x: scaledX, y: scaledY };
        let best = { idx: -1, dist: Infinity };
        objPositions.forEach((pt, i) => {
          const d = calculateDistance(clickPoint, pt);
          if (d < best.dist) {
            best.idx = i;
            best.dist = d;
          }
        });
        
        data.scaled_x = scaledX;
        data.scaled_y = scaledY;
        data.distance_from_target = distance;
        data.nearest_object_index = best.idx;
        data.nearest_object_distance = best.dist;
        data.correct_object_index = permPosition;
        data.is_nearest_correct = (best.idx === permPosition);
        if (data.is_nearest_correct) {
          window.spatialAlloCorrectCounter++;
        }
        data.cumulative_spatialallo_correct = window.spatialAlloCorrectCounter;
    } 
  }
};


//timeline.push(spatialAlloBindingTest);


