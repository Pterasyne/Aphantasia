        choices: ["Continuer"],
        data: {screen: "TAS_instructions"},
      };
      //timeline.push(TAS_instructions);

      var TASlikert_scale = [
        "Désaccord complet",
        "Désaccord relatif",
        "Ni accord ni désaccord",
        "Accord relatif",
        "Accord complet",
      ];

      var TAS = {
        type: jsPsychSurveyLikert,
        data: {screen: "TAS"},
        questions: [
            {prompt: "Souvent, je ne vois pas très clair dans mes sentiments.", name: '1TI', labels: TASlikert_scale, required: true},
