const objectAssociationTrial = {
    type: jsPsychSurveyText,
    questions: [{
        prompt: "Quel objet ou élément de l'environnement virtuel était le plus proche de ce mot lorsqu'il est apparu ?",
        name: "object_response",
        placeholder: ""
    }],
    on_finish: (data) => {
        const userInput = data.response.object_response.toLowerCase().trim();
        
        const allVariants = objects.flatMap(obj => 
            [obj.name, ...obj.synonyms].map(variant => ({
                object: obj.name,
                variant: variant.toLowerCase(),
                distance: levenshtein(userInput, variant.toLowerCase())
            }))
        );

        const topMatches = allVariants
            .sort((a, b) => a.distance - b.distance)
            .slice(0, 3); 

        const correctObject = objects[window.permPosition % objects.length].name;
        const bestMatch = topMatches[0];
        
        data.object_correct = bestMatch.object === correctObject;
        data.object_expected = correctObject;
        data.object_entered = bestMatch.variant;
        data.object_distance = bestMatch.distance;
        data.object_top3 = topMatches;
        if (data.object_correct) {
          window.objectDetailCorrectCounter++;
        }
        data.cumulative_detail_correct = window.objectDetailCorrectCounter;
    }
};
//timeline.push(objectAssociationTrial);


