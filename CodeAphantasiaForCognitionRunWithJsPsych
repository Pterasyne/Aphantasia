const VVIQ_block = {
  timeline: [VVIQ_instructions, VVIQ]
};

const AQ_block = {
  timeline: [AQ_instructions, AQ]
};

const TAS_block = {
  timeline: [TAS_instructions, TAS]
};

const questionnaires = [VVIQ_block, AQ_block, TAS_block];

const randomizedQuestionnaires = jsPsych.randomization.shuffle(questionnaires);

for (const questionnaire of randomizedQuestionnaires) {
  timeline.push(questionnaire);
}

timeline.push(general_instructions);

const testsToShuffle = [
  temporalseqBindingTest, 
  temporalroomBindingTest,
  spatialBindingTest, 
  spatialAlloBindingTest, 
  objectAssociationTrial
];

function generateBlock(index) {
  let prevSpatialEgo, prevSpatialAllo, prevTempSeq, prevTempRoom;

  return {
    timeline: [
      {
        type: jsPsychCallFunction,
        func: () => {
          prevSpatialEgo = window.spatialEgoCorrectCounter;
          prevSpatialAllo = window.spatialAlloCorrectCounter;
          prevTempSeq = window.temporalSeqCorrectCounter;
          prevTempRoom = window.temporalRoomCorrectCounter;
        }
      },
      freeRecallTrial,
      {
        timeline: jsPsych.randomization.shuffle([...testsToShuffle]),
        conditional_function: () => !window.shouldStop && !window.shouldSkipTests 
      }
    ],

    conditional_function: function() {
      return window.blockCount === index && !window.shouldStop;
    },
    
    on_timeline_finish: function() {
      if (window.spatialEgoCorrectCounter > prevSpatialEgo
       || window.spatialAlloCorrectCounter > prevSpatialAllo) {
        window.spatialCorrectCounter++;
      }

      if (window.temporalSeqCorrectCounter > prevTempSeq
       || window.temporalRoomCorrectCounter > prevTempRoom) {
        window.temporalCorrectCounter++;
      }

      window.blockCount++;
    }
  };
}

const allBlocks = []; // Probably a better way though
for (let i = 0; i < 44; i++) {
  allBlocks.push(generateBlock(i));
}

timeline.push(...allBlocks);

      var finalScreen = {
        type: jsPsychHtmlButtonResponse,
        stimulus: function() {
          return `
            <p>Merci beaucoup pour votre participation !</p>
            <br>
            <p>Vos données ne seront enregistrées que lorsque vous cliquerez sur le bouton 'Finir l'expérience' ; vous aurez alors accès au code Prolific.</p>
          `;
        },
        choices: ["Finir l'expérience"],
        data: function() {
          var tScore = calculateScoreForSurvey('TI', jsPsych.data.get().values()) 
          + calculateScoreForSurvey('TD', jsPsych.data.get().values()) 
          + calculateScoreForSurvey('TO', jsPsych.data.get().values());
          var aScore = calculateScoreForSurvey('AD', jsPsych.data.get().values())
          +calculateScoreForSurvey('AI', jsPsych.data.get().values())
          +calculateScoreForSurvey('AF', jsPsych.data.get().values())
          +calculateScoreForSurvey('AS', jsPsych.data.get().values())
          +calculateScoreForSurvey('AC', jsPsych.data.get().values());
          var vScore = calculateScoreForSurvey('VF', jsPsych.data.get().values())
          +calculateScoreForSurvey('VR', jsPsych.data.get().values())
          +calculateScoreForSurvey('VS', jsPsych.data.get().values())
          +calculateScoreForSurvey('VC', jsPsych.data.get().values());

          var tiScore = calculateScoreForSurvey('TI', jsPsych.data.get().values()) 
          var tdScore =  calculateScoreForSurvey('TD', jsPsych.data.get().values()) 
          var toScore =  calculateScoreForSurvey('TO', jsPsych.data.get().values());
          var adScore = calculateScoreForSurvey('AD', jsPsych.data.get().values())
          var aiScore = calculateScoreForSurvey('AI', jsPsych.data.get().values())
          var afScore = calculateScoreForSurvey('AF', jsPsych.data.get().values())
          var asScore = calculateScoreForSurvey('AS', jsPsych.data.get().values())
          var acScore = calculateScoreForSurvey('AC', jsPsych.data.get().values());
          var vfScore = calculateScoreForSurvey('VF', jsPsych.data.get().values())
          var vrScore = calculateScoreForSurvey('VR', jsPsych.data.get().values())
          var vsScore = calculateScoreForSurvey('VS', jsPsych.data.get().values())
          var vcScore = calculateScoreForSurvey('VC', jsPsych.data.get().values());
          
          window.concretudeStats = {
            Haut: 0,
            Moy: 0,
            Bas: 0,
            Inconnu: 0
          };
          
          if (window.foundWords && window.foundWords.length > 0) {
            window.foundWords.forEach(mot => {
              const niveau = concretudeMots[mot] || "Inconnu";
              window.concretudeStats[niveau]++;
            });
          }
          
        return {
            screen: "Final_Screen",
            tScore: tScore,
            aScore: aScore,
            vScore: vScore,
            tiScore: tiScore,
            tdScore: tdScore,
            toScore: toScore,
            adScore: adScore,
            aiScore: aiScore,
            afScore: afScore,
            asScore: asScore,
            acScore: acScore,
            vfScore: vfScore,
            vrScore: vrScore,
            vsScore: vsScore,
            vcScore: vcScore,

            correctTemporalSeqCount: window.temporalSeqCorrectCounter,
            correctTemporalRoomCount: window.temporalRoomCorrectCounter,
            correctSpatialEgoCount: window.spatialEgoCorrectCounter,
            correctSpatialAlloCount: window.spatialAlloCorrectCounter,
            correctTemporalCount: window.temporalCorrectCounter,
            correctSpatialCount: window.spatialCorrectCounter,
            correctObjectDetailCount: window.objectDetailCorrectCounter,
            wordsFounds: window.foundWords,
            totalFounds: window.foundWords.length,
            
            concretude_haut: window.concretudeStats.Haut,
            concretude_moy: window.concretudeStats.Moy,
            concretude_bas: window.concretudeStats.Bas,
            
            genreDetecte: window.gender,
            age: window.age,
            
            completionTime: Date.now() - jsPsych.getStartTime()
          };
        },
      };

timeline.push(finalScreen);
      

var exit_fullscreen = {
  type: jsPsychFullscreen,
  fullscreen_mode: false,
  delay_after: 0
};

timeline.push(exit_fullscreen);


jsPsych.run(timeline);
