      //timeline.push(VVIQ);

      function calculateScoreForSurvey(surveyName, data) {
        var totalScore = 0;
        data.forEach(function(trial) {
          if (trial.trial_type === 'survey-likert') {
            if (trial.response) {
              Object.keys(trial.response).forEach(function(key) {
                if (key.endsWith(surveyName) && key.startsWith('0')==false) { 
                  var responseValue = trial.response[key];
                  // Reversed items
                  if (key.startsWith('N')) {
                    responseValue = 4 - responseValue;  
                  };
                  if (key.endsWith("AD") || key.endsWith("AI") || key.endsWith("AF") || key.endsWith("AS") || key.endsWith("AC") ) { 
                    if (key.startsWith('N') && trial.response[key] > 1 || key.startsWith('N') == false && trial.response[key] <= 1 ) {
                            responseValue = 0
                        } else {
                            responseValue = -1
                        }
                  };
                  totalScore += responseValue + 1;
                }
              });
            }
          }
        });
        return totalScore;
      }
      
